

{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/profiles(.*)", "dest": "index.js" },
    { "src": "/artists(.*)", "dest": "index.js" },
    { "src": "/reviews(.*)", "dest": "index.js" },
   
    { "src": "/(.*)", "dest": "index.js" }
  ]
}

  